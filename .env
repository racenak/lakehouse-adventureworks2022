# Database
POSTGRES_DB=mage
POSTGRES_USER=mage
POSTGRES_PASSWORD=mage
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
MAGE_DATABASE_CONNECTION_URL=postgresql+psycopg2://mage:mage@postgres:5432/mage

# Redis
REDIS_URL=redis://redis:6379

# Scheduler (SIÊU QUAN TRỌNG - thiếu cái này là crash 100%)
